program DLLLoader;

uses
  Vcl.Forms,
  uDLLLoader in 'uDLLLoader.pas' {frmDLLLoader},
  uMultiDLLLoader in 'uMultiDLLLoader.pas' {frmDLLLoader1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmDLLLoader, frmDLLLoader);
  Application.CreateForm(TfrmDLLLoader1, frmDLLLoader1);
  Application.Run;
end.
